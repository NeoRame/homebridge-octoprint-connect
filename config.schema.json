{
    "pluginAlias": "Octoprint-Temp",
    "pluginType": "accessory",
    "singular": false,
    "schema": {
      "type": "object",
      "properties": {
        "name": {
          "title": "Printer-Name",
          "type": "string",
          "required": true
        },
        "server": {
            "title": "Octoprint server address. Optional if it is already specified in octopi.local",
            "type": "string",
            "required": false
        },
        "api_key": {
            "title": "Octoprint API-Key. Can be found in the Octoprint settings",
            "type": "string",
            "required": true
        },
        "debug": {
            "title": "Use Debug",
            "type": "boolean"
        }
      }
    }
  }